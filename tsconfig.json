{
  "extends": "./strict.json",
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "@atos6/tsconfig/tsconfig.json",
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "declaration": true,
    "esModuleInterop": true,
    "isolatedModules": false,
    "jsx": "react",
    "module": "CommonJS",
    "moduleResolution": "node",
    "noEmit": true,
    "noEmitOnError": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "target": "ESNext"
  }
}
